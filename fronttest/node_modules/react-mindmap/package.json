{
  "name": "react-mindmap",
  "version": "2.1.6",
  "description": "React component for rendering mind maps",
  "license": "MIT",
  "contributors": [
    {
      "name": "Angelo Gazzola",
      "email": "angelo@nglgzz.com",
      "url": "https://github.com/nglgzz"
    }
  ],
  "files": [
    "dist",
    "src/parser"
  ],
  "main": "dist/index.js",
  "bin": {
    "react-mindmap-parse": "./src/parser/index.js"
  },
  "repository": "learn-anything/react-mindmap",
  "scripts": {
    "parse": "node src/parser/index.js",
    "lint": "eslint .; sass-lint sass/* -v",
    "pre-build": "rollup -c && uglifyjs dist/d3.js -c -m -o dist/d3.min.js",
    "build": "npm run pre-build && webpack && cp dist/index.js test/react-mindmap.js",
    "prepare": "npm run build && rm dist/d3.min.js dist/d3.js",
    "test": "npm run build && cp dist/index.js test/react-mindmap.js && cd test && static-server -p 3000"
  },
  "dependencies": {
    "d3": "^4.9.1",
    "fs-walk": "0.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "static-server": "^2.0.5"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^7.4.0",
    "http-server": "^0.10.0",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.0.5",
    "rollup": "^0.50.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.2",
    "uglify-js": "^3.1.3",
    "webpack": "^3.6.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "keywords": [
    "react-component",
    "visualization",
    "mindnode",
    "mindmap",
    "react"
  ]
}
